<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> <mapper namespace="com.etop.website.dao.WebNewsDao">         <select id="searchNews" resultType="com.etop.website.bean.News" parameterType="Map">         SELECT * from etop_news where total_type="1" and state = '1' order by created_at desc                </select>                <select id="searchNews2" resultType="com.etop.website.bean.News" parameterType="String">         	SELECT * from etop_news where total_type="1"  and state = '1' and          	         	park_id in (select id from etop_park where park_group_id=#{parkGroupId})         	         	order by created_at desc                </select>                <select id="searchInfo" resultType="com.etop.website.bean.News" parameterType="Map">         SELECT * from etop_news where total_type="2" and state = '1' order by created_at desc        </select>                <select id="searchNewsInfo" resultType="com.etop.website.bean.News" parameterType="String">         SELECT * from etop_news where id=#{id}        </select>                        <select id="listAll" resultType="com.etop.website.bean.News" parameterType="Map">         SELECT * FROM etop_news         WHERE total_type = "1"         order by date desc        limit ${page},${pageSize}                                          </select>                        <select id="getparkName" resultType="String" parameterType="String">         		        SELECT park_name		        FROM etop_park		        WHERE id = #{id}                                         </select></mapper>