<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> <mapper namespace="com.etop.website.dao.OlmerchantsDao">            <select id="searchWebpark" resultType="com.etop.website.bean.Webpark" parameterType="com.etop.website.bean.Webpark">			SELECT				*			FROM				etop_park			where				activated = '1'						<if test="parkGroupId!=null and parkGroupId!=''">				and park_group_id = #{parkGroupId}			</if>				        </select>        <select id="searchInfo" resultType="com.etop.website.bean.Webpark" parameterType="String">                 SELECT *			from etop_park			WHERE id=#{id}        </select>        <select id="getCity" resultType="com.etop.website.bean.Webpark" parameterType="com.etop.website.bean.Webpark">            SELECT *			from etop_park			WHERE city=#{city}			<if test="parkGroupId!=null and parkGroupId!=''">				and park_group_id = #{parkGroupId}			</if>        </select></mapper>