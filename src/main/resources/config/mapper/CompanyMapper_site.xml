<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> <mapper namespace="com.etop.website.dao.CompanyDao">            <select id="searchInfo" resultType="com.etop.website.bean.Company" parameterType="String">        SELECT * from etop_company where company_id=#{company_id}        </select>                <select id="searchname" resultType="com.etop.website.bean.Company" parameterType="String">        SELECT company_name from etop_company where company_id=#{companyId}        </select>    </mapper>